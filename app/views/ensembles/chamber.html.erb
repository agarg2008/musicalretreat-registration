<h1>Chamber Music Choices</h1>

<ul>
<li>Ensemble primary id <%= @ensemble_primary.id %>
<li>Primary instrument id <%= @primary_instrument_id %>
<li>Num mmr <%= @num_mmr %>
<li>Num pre <%= @num_prearranged %>
</li>

<%= form_for (:ensemble_primary, url => {:action => :create_chamber}) do |form| %>
   <%= form.fields_for :mmr_chambers do |mmr| %>
      <%= render :partial => :mmr_chamber %>
   <% end %>
   <%= form.fields_for :preselect_chambers do |preselect| %>
      <%= render :partial => :prearranged_chamber %>
   <% end %>
<%= form.submit 'Submit' %>
<% end %>
